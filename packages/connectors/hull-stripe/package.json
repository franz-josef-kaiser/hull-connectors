    "file-loader": "^1.1.11",
    "flat": "^4.0.0",
    "glob": "^7.1.2",
    "happypack": "^5.0.0",
    "hull": "0.13.16",
    "ioredis": "^3.2.2",
    "jquery": "^3.3.1",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "node-sass": "^4.9.0",
    "object-mapper": "^5.0.0",
    "passport-stripe": "^0.2.2",
    "peity": "^3.3.0",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.5",
